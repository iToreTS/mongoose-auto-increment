[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Tore (iToreTS)
[access "refs/for/refs/*"]
	read = group user/Tore (iToreTS)
	push = group user/Tore (iToreTS)
	label-Code-Review = -2..+2 group user/Tore (iToreTS)
	label-Verified = -1..+1 group user/Tore (iToreTS)
	rebase = group user/Tore (iToreTS)
	removeReviewer = group user/Tore (iToreTS)
	submit = group user/Tore (iToreTS)
[access "refs/heads/*"]
	read = group user/Tore (iToreTS)
	create = group user/Tore (iToreTS)
	push = +force group user/Tore (iToreTS)
	pushMerge = group user/Tore (iToreTS)
[access "refs/tags/*"]
	push = +force group user/Tore (iToreTS)
	pushTag = group user/Tore (iToreTS)
	pushSignedTag = group user/Tore (iToreTS)
[submit]
	action = merge always
